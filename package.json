{
  "name": "keep-in-touch-be",
  "version": "1.0.0",
  "main": "dist/index.js",
  "_moduleAliases": {
    "~models": "./dist/models",
    "~services": "./dist/services",
    "~controllers": "./dist/controllers",
    "~middleware": "./dist/middleware",
    "~utils": "./dist/utils",
    "~config": "./dist/config",
    "~routes": "./dist/routes",
    "~validation": "./dist/validation",
    "~types": "./dist/types",
    "~templates": "./dist/templates"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "dotenv -e .env.production node dist/server.js",
    "dev": "dotenv -e .env.development tsx watch --require tsconfig-paths/register src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:auth": "jest --testPathPatterns=auth",
    "test:auth:controllers": "jest --testPathPatterns=controllers/authController",
    "test:auth:services": "jest --testPathPatterns=services/authService",
    "test:users": "jest --testPathPatterns=users",
    "test:users:controllers": "jest --testPathPatterns=controllers/usersController",
    "test:users:services": "jest --testPathPatterns=services/userService",
    "test:controllers": "jest --testPathPatterns=controllers",
    "test:services": "jest --testPathPatterns=services",
    "test:middleware": "jest --testPathPatterns=middleware",
    "test:utils": "jest --testPathPatterns=utils",
    "test:integration": "jest --testPathPatterns=integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:imports": "prettier --write src/**/*.ts --plugin=@trivago/prettier-plugin-sort-imports",
    "format:check": "prettier --check src/**/*.ts",
    "format:tests": "prettier --write src/__tests__/**/*.ts",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Alex Borovskoy",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "html-to-text": "^9.0.5",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.5",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.5",
    "winston": "^3.17.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.3",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/module-alias": "^2.0.4",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.1.0",
    "@types/nodemailer": "^7.0.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "dotenv-cli": "^9.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^15.2.7",
    "module-alias": "^2.2.3",
    "mongodb-memory-server": "^10.2.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3"
  }
}
